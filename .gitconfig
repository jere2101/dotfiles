[color]
    ui = auto

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = cyan

[color "diff"]
    meta = yellow
    frag = cyan  # Lines infos
    old = red  # Deletions
    new = green  # Additions
    whitespace = red reverse

[color "status"]
    added = green
    changed = yellow
    untracked = red

[help]
    # Automatically correct and execute mistyped commands
    autocorrect = 1
[user]
  name = jeremie.thoennissen
[core]
	editor = vim
[pull]
	rebase = false
[push]
	autoSetupRemote = true
